<?xml version="1.0" encoding="UTF-8"?>
<ComponentTypeSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../Schemas/ComponentTypeSet.xsd">
    <ComponentType Name="FbaIir" Mapping="ByteControlWithLength:%1 %2 %3 params %4">
        <Component Name="switch" Type="SwitchParam"/>
        <IntegerParameter Name="sw_flag" Size="16"
            Description="Module version. Default=0 (IIR 1.0)"/>
        <EnumParameter Name="order" Size="16" Description="Order of the filter">
            <ValuePair Literal="Second" Numerical="2"/>
            <ValuePair Literal="Fourth" Numerical="4"/>
            <ValuePair Literal="Sixth" Numerical="6"/>
            <ValuePair Literal="Eighth" Numerical="8"/>
        </EnumParameter>
        <ParameterBlock Name="biquad" ArrayLength="4">
            <IntegerParameter Name="a1" Size="16" Signed="true"/>
            <IntegerParameter Name="b1" Size="16" Signed="true"/>
            <IntegerParameter Name="a2" Size="16" Signed="true"/>
            <IntegerParameter Name="b2" Size="16" Signed="true"/>
            <IntegerParameter Name="a0" Size="16" Signed="true"/>
        </ParameterBlock>
    </ComponentType>
</ComponentTypeSet>